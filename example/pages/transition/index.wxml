<mu-cell-group inset>
  <mu-cell
    title="Fade"
    isLink
    data-item="fade"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Fade up"
    isLink
    data-item="fade-up"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Fade down"
    isLink
    data-item="fade-down"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Fade Left"
    isLink
    data-item="fade-left"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Fade Right"
    isLink
    data-item="fade-right"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Slide up"
    isLink
    data-item="slide-up"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Slide down"
    isLink
    data-item="slide-down"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Slide Left"
    isLink
    data-item="slide-left"
    bind:tap="openTransition"
  ></mu-cell>
  <mu-cell
    title="Slide Right"
    isLink
    data-item="slide-right"
    bind:tap="openTransition"
  ></mu-cell>
</mu-cell-group>



<mu-transition
  show="{{show}}"
  name="{{name}}"
  custom-class="block"
  enter-class="van-enter-class"
  enter-active-class="van-enter-active-class"
  leave-active-class="van-leave-active-class"
  leave-to-class="van-leave-to-class"
  bind:beforeEnter="beforeEnter"
  bind:enter="enter"
  bind:afterEnter="afterEnter"
  bind:beforeLeave="beforeLeave"
  bind:leave="leave"
  bind:afterLeave="afterLeave"
/>