.mu-transition {
  transition-timing-function: ease;
  transition-duration: 300ms;
}
.fade-up-enter,
.fade-up-leave-to {
  opacity: 0;
  transition-property: opacity, transform;
}
.fade-up-enter-active,
.fade-up-leave-active {
  transition-property: opacity, transform;
}
.fade-up-enter-to,
.fade-up-leave {
  transform: translate3d(100px, 10px, 0);
  transition-property: opacity, transform;
}
