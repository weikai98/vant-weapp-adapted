<wxs
  module="utils"
  src="../common/wxs/utils.wxs"
></wxs>


<mu-transition
  show="{{show}}"
  name="{{transitionName}}"
  duration="{{ duration }}"
  custom-style="z-index: {{zIndex}}"
  custom-class="{{ utils.bem('modal', [{transparent}]) }}"
  bind:tap="onMaskClick"
  catch:touchmove="{{lockScroll ? 'touchmove' : ''}}"
>
  <slot />
</mu-transition>